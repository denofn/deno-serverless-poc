{
  "compilerOptions": {
    "types": ["./types"],
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "http://*": [".deno/deps/http/*"],
      "https://*": [".deno/deps/https/*"]
    },
    "plugins": [{ "name": "deno_ls_plugin" }],
    "pretty": true,
    "resolveJsonModule": true,
    "target": "esnext"
  },
  "include": ["./**/*.ts"]
}
